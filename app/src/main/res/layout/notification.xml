<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:paddingTop="@dimen/padding_5"
    android:paddingBottom="@dimen/padding_5"
    android:layout_height="wrap_content"
    android:minHeight="@dimen/my_reservation_item_size"
    android:background="@color/white_color">


    <ImageView
        android:layout_marginRight="@dimen/margin_10"
        android:id="@+id/swipe_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:background="@android:color/transparent"
        android:clickable="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="center"
        android:src="@drawable/next_arrow"


        android:textSize="10dp" />

    <TextView

        android:id="@+id/admin_list_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/margin_10"

        android:layout_marginRight="@dimen/margin_5"

        android:layout_marginTop="@dimen/margin_5"

        android:layout_toLeftOf="@id/swipe_button2"
        android:maxLines="3"
        android:text="iou iouewrwqerrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrj"
        android:textColor="@color/black_color"
        android:textSize="@dimen/text_size16" />
</RelativeLayout>