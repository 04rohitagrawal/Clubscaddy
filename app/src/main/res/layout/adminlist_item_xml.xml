<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/admin_list_relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/margin_10"
    android:background="@color/white_color"
    android:orientation="vertical"
    android:padding="@dimen/padding_5" >

    <TextView
        android:text="fdsfsdflj"
        android:id="@+id/admin_list_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:textColor="@color/black_color"
        android:textSize="@dimen/text_size16" />

    <TextView
         android:text="fdsfsdflj"
        android:id="@+id/admin_list_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/admin_list_name"
        android:layout_marginTop="@dimen/margin_5"
        android:singleLine="true"
        android:textColor="@color/black_color"
        android:textSize="@dimen/text_size16" />

    <TextView
         android:text="fdsfsdflj"
        android:id="@+id/admin_list_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/admin_list_phone"
        android:layout_marginTop="@dimen/margin_5"
        android:singleLine="true"
        android:textColor="@color/black_color"
        android:textSize="@dimen/text_size16" />

    <ImageView
        android:id="@+id/admin_list_nextArrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerInParent="true"
        android:background="@drawable/next_arrow"
        android:contentDescription="@string/app_name"
        android:focusable="false"
        android:focusableInTouchMode="false" />

</RelativeLayout>