<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/manage_coaches_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="@dimen/margin_10"
        android:background="@drawable/whitecolor_background"
        android:drawableRight="@drawable/next_arrow"
        android:padding="10dp"
        android:text="Manage coaches"
        android:textColor="@android:color/black"
        android:textSize="@dimen/text_size14" />

    <TextView
        android:id="@+id/manage_admin_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="@dimen/margin_5"
        android:background="@drawable/whitecolor_background"
        android:drawableRight="@drawable/next_arrow"
        android:padding="10dp"
        android:text="Manage admins"
        android:textColor="@android:color/black"
        android:textSize="@dimen/text_size14" />

    <TextView
        android:id="@+id/manage_member_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="@dimen/margin_5"
        android:background="@drawable/whitecolor_background"
        android:drawableRight="@drawable/next_arrow"
        android:padding="10dp"
        android:text="Manage Memebers"
        android:textColor="@android:color/black"
        android:textSize="@dimen/text_size14" />

    <TextView
        android:id="@+id/manage_clublogo_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="@dimen/margin_5"
        android:background="@drawable/whitecolor_background"
        android:drawableRight="@drawable/next_arrow"
        android:padding="10dp"
        android:text="Managing club logo"
        android:textColor="@android:color/black"
        android:textSize="@dimen/text_size14" />
    <TextView
        android:id="@+id/manage_club_message"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="@dimen/margin_5"
        android:background="@drawable/whitecolor_background"
        android:drawableRight="@drawable/next_arrow"
        android:padding="10dp"
        android:text="Managing club Message"
        android:textColor="@android:color/black"
        android:textSize="@dimen/text_size14" />

    <TextView
        android:id="@+id/manage_court_reservation_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="@dimen/margin_5"
        android:background="@drawable/whitecolor_background"
        android:drawableRight="@drawable/next_arrow"
        android:padding="10dp"
        android:text="Manage court reservation"
        android:textColor="@android:color/black"
        android:textSize="@dimen/text_size14" />

    <TextView
        android:id="@+id/general_setting"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="@dimen/margin_5"
        android:background="@drawable/whitecolor_background"
        android:drawableRight="@drawable/next_arrow"
        android:padding="10dp"
        android:text="General settings"
        android:textColor="@android:color/black"
        android:textSize="@dimen/text_size14" />
<TextView
        android:id="@+id/manage_sport_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="@dimen/margin_5"
        android:background="@drawable/whitecolor_background"
        android:drawableRight="@drawable/next_arrow"
        android:padding="10dp"
        android:text="Manage Sport Type"
        android:textColor="@android:color/black"
        android:textSize="@dimen/text_size14" />
<TextView
        android:id="@+id/manage_rating"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="@dimen/margin_5"
        android:background="@drawable/whitecolor_background"
        android:drawableRight="@drawable/next_arrow"
        android:padding="10dp"
        android:text="Manage Rating"
        android:textColor="@android:color/black"
        android:textSize="@dimen/text_size14" />
</LinearLayout>